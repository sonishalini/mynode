<% include header %>
<% include nav %>

		<div id="page">
			<div id="content">
				<div class="box">
					
					<center>
							<h2 class="agileits-sear-head">Register here!!!</h2>
							<font color="orange"><%= output %></font>
							<form method="post" action="/register">
								<table cellspacing="20"  >
									<tr>
										<td>Name</td>
										<td><input type="text" name="name" placeholder="Enter name" /></td>
									</tr>
									<tr>
										<td>Username/Email</td>
										<td><input type="text" name="email" placeholder="Enter Email" /></td>
									</tr>
									<tr>
										<td>Password</td>
										<td><input type="password" name="password" placeholder="Enter password" /></td><br>
									</tr>
									<tr>
										<td>Mobile</td>
										<td><input type="text" name="mobile" placeholder="Enter mobile number" /></td><br>
									</tr>
									<tr>
										<td>Address</td>
										<td>
											<textarea rows='5' cols='27' name="address"
												placeholder="Enter address"></textarea>
										</td>
									</tr>
									<tr>
										<td>City</td>
										<td>
											<select style="width:235px;" name="city">
												<option>Select city</option>
												<optgroup label="Madhya Pradesh">
													<option>Indore</option>
													<option>Mansor</option>
													<option>Dhar</option>
													<option>Dewas</option>
													<option>Bhopal</option>
													<option>Ujjain</option>
												</optgroup>
												<optgroup label="Maharastra">
													<option>Mumbai</option>
													<option>Pune</option>
													<option>Jalgoan</option>
													<option>Bhusawal</option>
													<option>Mahableshwar</option>
													<option>Nasik</option>
												</optgroup>
											</select>
										</td>
									</tr>
									<tr>
										<td>Gender</td>
										<td>
											<input type="radio" name="gender" value="male" /> Male &nbsp;&nbsp;
											<input type="radio" name="gender" value="female" /> Female
										</td>
									</tr>
									<tr>
										<td>
											<input type="submit" value="Register"/>
										</td>
									</tr>
								</table>
							</form>
						</center>
						</div>
				</div>
		</div>



